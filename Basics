        import java.util.Scanner;
public class basicpattern {

    public static void main(String[] args) {
         Scanner input=new Scanner(System.in);
        int N=input.nextInt();
        for(int i=1;i<=N;i++)
        {
            for(int j=1;j<=N-i+1;j++)
            {
                System.out.print(j);
            }
            System.out.println(" ");
        }
    }
}
  
